<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th>#</th>
        <th>Country</th>
        <th>City</th>
        <th>Street</th>
        <th>Street No</th>
        <th>Zip Code</th>
        <th>Latitude</th>
        <th>Longitude</th>
      </tr>
    </thead>
    <tbody>
      <% @contact_addresses.each_with_index do |address, index| %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= address.country %></td>
          <td><%= address.city %></td>
          <td><%= address.street %></td>
          <td><%= address.street_no %></td>
          <td><%= address.zip_code %></td>
          <td><%= address.latitude %></td>
          <td><%= address.longitude %></td>
        </tr> 
      <% end %>
    </tbody>
  </table>
</div>
